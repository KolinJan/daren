/* pages/register-merchant/lregister-merchant.wxss */

.item-frame{
  border-bottom: 1px solid #e6e6e6;
  padding: 10px 5px;

}
.item-label{
    text-align: center;
    width: 20%;
    float: left;
}
.v-code{
    width: 25%;
    float: left;
}
.v-code view{
    font-size: 13px;
    color: #fff;
    background-color: #ff6537;
    text-align: center;
    border-radius: 3px;
    padding:5px;

}
.item-input{
    width: 55%;
    float: left;
}
.register-button-frame{
    text-align: center;
    position: absolute;
    top: 200px;
    width: 100%;
}
.register-button-frame view{
    background-color: #ff6537;
    padding: 10px 15px;
    color: #fff;
    display: inline-block;
    width: 35%;
    border-radius: 3px;
}